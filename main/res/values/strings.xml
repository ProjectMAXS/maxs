    <string name="perm_group_maxs_label">MAXS Permissions</string>
    <string name="perm_group_maxs_desc">access and use the Modular Android XMPP Suite (MAXS)</string>
    <string name="perm_use_main_label">Use MAXS Main</string>
    <string name="perm_use_main_desc">Allow generic access to the Main component of MAXS.</string>
    <string name="perm_use_main_as_module_label">Use MAXS Main as Module</string>
    <string name="perm_use_main_as_module_desc">Allow access to part of the MAXS Main that is required to act as MAXS Module.</string>
    <string name="perm_use_main_as_transport_label">Use MAXS Main as Transport</string>
    <string name="perm_use_main_as_transport_desc">Allow access to part of the MAXS Main that is required to act as MAXS Transport.</string>
    <string name="perm_use_module_label">Use MAXS Module</string>
    <string name="perm_use_module_desc">Allow access to MAXS Modules.</string>
    <string name="perm_use_transport_label">Use MAXS Transport</string>
    <string name="perm_use_transport_desc">Allow access to MAXS Transports.</string>
    <string name="perm_use_filewrite_label">Use MAXS FileWrite</string>
    <string name="perm_use_filewrite_desc">Allow the usage of the MAXS Module FileWrite. This includes reading, writing and deleting files of the devices storage.</string>
    <string name="perm_use_fileread_label">Use MAXS FileRead</string>
    <string name="perm_use_fileread_desc">Allow the usage of the MAXS Module FileRead.</string>
    <string name="perm_use_read_contacts_label">Use MAXS Module ContactsRead</string>
    <string name="perm_use_read_contacts_desc">Allow the usage of the MAXS Module ContactsRead. This includes read access to all contacts.</string>
    <string name="perm_use_write_sms_label">Use MAXS SmsWrite</string>
    <string name="perm_use_write_sms_desc">Allow the usage of the MAXS Module SmsWrite.</string>
    <string name="perm_use_incoming_filetransfer_service_label">Use MAXS Incoming Filetransfer Service</string>
    <string name="perm_use_incoming_filetransfer_service_desc">Allow to use the MAXS components for incoming filetransfers.</string>
    <string name="perm_use_outgoing_filetransfer_service_label">Use MAXS Outgoing Filetransfer Service</string>
    <string name="perm_use_outgoing_filetransfer_service_desc">Allow to use the MAXS components for outgoing filetransfers.</string>
    <string name="perm_use_phonestateread_service_label">Use MAXS Phonestateread Service</string>
    <string name="perm_use_phonestateread_service_desc">Allow to use the MAXS Phonestateread to read the state of the device.</string>
</resources>