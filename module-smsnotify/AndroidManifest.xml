<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="org.projectmaxs.module.smsnotify"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-permission android:name="android.permission.RECEIVE_SMS"/>
    <uses-permission android:name="org.projectmaxs.permission.USE_MAIN"/>
    <permission android:name="org.projectmaxs.permission.USE_MODULE"/>
    <uses-sdk android:minSdkVersion="8"/>
    <application android:label="@string/app_name">
        <receiver android:name=".RegisterModuleReceiver"
            android:label="TODO" android:permission="org.projectmaxs.permission.USE_MODULE">
            <intent-filter>
                <action android:name="org.projectmaxs.REGISTER"/>
            </intent-filter>
        </receiver>
        <receiver android:name=".SMSReceiver">
            <intent-filter>
                <!-- Receiver priority is set to SYSTEM_HIGH + 10 -->
                <action android:name="android.provider.Telephony.SMS_RECEIVED" android:priority="1010" />
            </intent-filter>
        </receiver>
    </application>

</manifest> 
